<h3> New Track </h3>
<%= @track.errors.first %>
<%= form_for @track do |f| %>
  <b> Name: </b>  <%= f.text_field :name %>
  <b> Album: </b>  <%= f.text_field :album_id %>
  <b> Media Type: </b>  <%= f.text_field :media_type_id %>
  <b> Genre: </b>  <%= f.text_field :genre_id %>
  <b> Composer: </b>  <%= f.text_field :composer %>
  <b> Milliseconds: </b>  <%= f.text_field :milliseconds %>
  <b> Size (bytes): </b>  <%= f.text_field :bytes %>
  <b> Unit Price: </b>  <%= f.text_field :unit_price %>
  <%= f.submit 'Save' %>
<% end %>
